{
  "expo": {
    "name": "only-cncs-app",
    "slug": "only-cncs-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adapt.png",
    "scheme": "onlycncsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/ios.png",
      "resizeMode": "contain",
      "backgroundColor": "#004146"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/adapt.png",
        "backgroundColor": "#004146"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adapt.png"
    },
  
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#004146",
      "dark": {
        "image": "./assets/images/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#004146"
      }
    }
  }
}
